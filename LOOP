//for

const powerlifting = ['supino', 'terra','agachamento'];

for(let pedidas = 0; pedidas < powerlifting.length; pedidas++ ) {
console.log(` Para realizar o esporte de porwerlifting vocÃª precisa realizar o movimento: ${powerlifting[pedidas]} `);
};

